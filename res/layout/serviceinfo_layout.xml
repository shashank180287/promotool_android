<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    <TextView android:id="@+id/movie_name"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:textSize="21sp"
         android:text="@string/empty"
         />
    <TextView android:id="@+id/offer"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:text="@string/empty"
         />
    <TableLayout android:id="@+id/tablelayout" android:layout_width="wrap_content" android:layout_height="wrap_content">
 
  		<TableRow android:id="@+id/TableRowTiming" android:layout_width="wrap_content" android:layout_height="wrap_content">
 
    		<TextView android:id="@+id/timing_head" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/timing"></TextView>
    		<TextView android:id="@+id/timing_value" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/empty"></TextView>
 
  		</TableRow>
 
  		<TableRow android:id="@+id/TableRowplace" android:layout_width="wrap_content" android:layout_height="wrap_content">
 
    		<TextView android:id="@+id/place_head" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/place"></TextView>
    		<TextView android:id="@+id/place_value" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/empty"></TextView>
 
  		</TableRow>
 
	</TableLayout>
	<Button android:id="@+id/Button01" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/interested"></Button>
</LinearLayout>